{
  "name": "code-naming-artiface",
  "displayName": "⚡️ 命名神器",
  "description": "变量命名瞬间搞定，让创意源源不断！",
  "version": "0.0.1",
  "publisher": "无穷369",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "变量命名",
    "命名",
    "变量"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "bugs": {
    "url": "https://github.com/zy598586050",
    "email": "598586050@qq.com"
  },
  "homepage": "https://github.com/zy598586050",
  "repository": {
    "type": "git",
    "url": "https://github.com/zy598586050/code-naming-artiface.git"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-naming-artiface.showNameList",
        "title": "⚡️ 命名神器"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "code-naming-artiface.showNameList",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "code-naming-artiface.showNameList",
        "key": "Ctrl+Shift+V",
        "mac": "Cmd+Shift+V",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.5.1"
  }
}